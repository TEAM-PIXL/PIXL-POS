<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.VBox?>

<VBox fx:controller="com.example.pixlpos.controllers.adminconsole.AdminConsoleController"
      xmlns:fx="http://javafx.com/fxml" alignment="CENTER" spacing="10">
    <padding>
        <Insets top="20" right="20" bottom="20" left="20"/>
    </padding>

    <Button fx:id="manageUsersButton" text="Manage Users" onAction="#onManageUsersButtonClick"/>
    <Button fx:id="manageMenuButton" text="Manage Menu" onAction="#onManageMenuButtonClick"/>


    <Button fx:id="logoutButton" text="Logout" onAction="#onLogoutButtonClick" />

</VBox>
